package com.test;

public class Employee {
	private String name;
	private int age;
	private String grade;
	
	Employee(){
		System.out.println("Employee constructor");
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getGrade() {
		return grade;
	}
	public void setGrade(String grade) {
		this.grade = grade;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "Employee - name : " + name + " age: " + age + " grade : " + grade +"\n"+ super.toString();
	}

	public void initMehtod() {
		System.out.println("InitMethod call from bean");
	}
	
	public void dMethod() {
		System.out.println("Destory method call");
	}
}
