	@GET
	@Produces(MediaType.TEXT_XML)
	public String sayHello() {
		return null;
		
	}
	@GET
	@Produces(MediaType.APPLICATION_JSON)
	public String sayHelloJason() {
		return null;
	}
	
	@GET
	@Produces(MediaType.TEXT_HTML)
	public String sayHellHtml() {
		return null;
	}
}
